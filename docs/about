    <script>
        // Smooth scrolling for navigation links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function(e) {
                e.preventDefault();
                
                const targetId = this.getAttribute('href');
                if(targetId === '#') return;
                
                const targetElement = document.querySelector(targetId);
                if(targetElement) {
                    window.scrollTo({
                        top: targetElement.offsetTop - 80,
                        behavior: 'smooth'
                    });
                }
            });
        });
        
        // CTA button interactions
        const ctaButtons = document.querySelectorAll('.cta-button');
        ctaButtons.forEach(button => {
            button.addEventListener('click', function() {
                if(this.textContent.includes('Download') || this.textContent.includes('Start') || this.textContent.includes('Try') || this.textContent.includes('Get') || this.textContent.includes('Save')) {
                    alert("Thank you for your interest in Oraclize! In a real implementation, this would redirect to the App Store or Google Play.");
                }
            });
        });
        
        // Fade-in animation on scroll
        const fadeElements = document.querySelectorAll('.fade-in');
        
        const appearOnScroll = new IntersectionObserver(function(entries) {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.classList.add('visible');
                }
            });
        }, { threshold: 0.1 });
        
        fadeElements.forEach(element => {
            appearOnScroll.observe(element);
        });
        
        // Add floating effect to some elements
        const addFloatingEffect = () => {
            const elements = document.querySelectorAll('.tradition-card, .premium-card');
            elements.forEach((el, index) => {
                // Stagger the animation
                el.style.animationDelay = `${index * 0.2}s`;
            });
        };
        
        // Initialize when page loads
        window.addEventListener('DOMContentLoaded', addFloatingEffect);
    </script>
</body>
</html>
